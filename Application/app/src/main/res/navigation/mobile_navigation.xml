<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    tools:ignore="UnusedNavigation"
    app:startDestination="@id/discoverCapsuleFragment">
    <fragment
        android:id="@+id/discoverCapsuleFragment"
        android:name="com.example.group_w01_07_3.ui.discover.DiscoverCapsuleFragment"
        android:label="fragment_discover_capsule"
        tools:layout="@layout/fragment_discover_capsule" />
    <fragment
        android:id="@+id/createCapsuleFragment"
        android:name="com.example.group_w01_07_3.ui.create.CreateCapsuleFragment"
        android:label="fragment_create_capsule"
        tools:layout="@layout/fragment_create_capsule" />
    <fragment
        android:id="@+id/accountFragment"
        android:name="com.example.group_w01_07_3.ui.account.AccountFragment"
        android:label="AccountFragment" >
        <action
            android:id="@+id/action_accountFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_accountFragment_to_changePassword2"
            app:destination="@id/changePassword2" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.group_w01_07_3.EditProfileFragment"
        android:label="EditProfileFragment" />
    <activity
        android:id="@+id/changePassword2"
        android:name="com.example.group_w01_07_3.ChangePassword"
        android:label="ChangePassword" />
</navigation>